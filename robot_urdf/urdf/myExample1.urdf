<?xml version="1.0"?>

<robot name="Robot1">		
	<link name="link_chassis">
    <!-- body -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.3 0.07"/>
      </geometry>
    </visual>
    <!-- caster front -->
    <visual name="caster_front_visual">
      <origin rpy=" 0 0 0" xyz="0.2 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

   <link name="link_right_wheel">
    <visual name="link_right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
       	<cylinder length="0.04" radius="0.1"/>
      </geometry>
    </visual>
  </link>

   <link name="link_left_wheel">
    <visual name="link_left_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.1"/>
      </geometry>
    </visual>
  </link>
  
  <link name = "Rotating_base">
   <visual name = "Rotating_base_visual">
    <origin rpy="0 0 0" xyz="0 0 0.05" />
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
   </visual>
  
  </link>
  
  <link name = "Link1">
   <visual name = "Link1_visual">
    <origin rpy="0 0 0" xyz="0 0 0.25" />
      <geometry>
        <box size="0.05 0.05 0.5"/>
      </geometry>
   </visual>
  
  </link>
  
  
  <link name = "Link2">
   <visual name = "Link1_visual">
    <origin rpy="0 0 0" xyz="0.05 0 0.2" />
      <geometry>
        <box size="0.05 0.05 0.5"/>
      </geometry>
   </visual>
  
  </link>
  
  
  
  
  


  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="-0.05 0.15 0"/>
    <child link="link_right_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="-0.05 -0.15 0"/>
    <child link="link_left_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  <joint name="joint_Rotating_base" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0.035"/>
    <child link="Rotating_base"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  
  <joint name="joint_Link1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.1"/>
    <child link="Link1"/>
    <parent link="Rotating_base"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
    <limit effort="10000" velocity="1000" lower = "-0.785" upper = "0.785"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  <joint name="joint_Link2" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.45"/>
    <child link="Link2"/>
    <parent link="Link1"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
    <limit effort="10000" velocity="1000" lower = "-0.785" upper = "0.785"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

</robot>
